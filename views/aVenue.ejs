<div class ="mt-4 card card-body"  style="border:1px solid black">
        <h1 class="mt-4"  style =" text-align: center"><i class="fas fa-plus-circle"></i>  Add Venue</h1>
        <p class=" mt-3 lead mb-1" style =" text-align: center">Select venues that you wish to add to Database</p>
        
</div>
<div class="row mt-3">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <% include ./partials/messages %>
      <form action="/settings/aVenue" method="post">

	    <div class="form-group">
          <label for="roomNo">Venue Number</label>
          <input
            type="roomNo"
            id="roomNo"
            name="roomNo"
            class="form-control"
            placeholder="Enter venue number"
            value="<%= typeof roomNo != 'undefined' ? roomNo : '' %>"
          >
        </div>
        <div class="form-group">
          <label for="capacity">Capacity</label>
          <input
            type="capacity"
            id="capacity"
            name="capacity"
            class="form-control"
            placeholder="Enter capacity"
            value=""<%= typeof capacity != 'undefined' ? capacity : '' %>"
          >
        </div>

        <div class="form-group">
          <label for="projector">Projector</label>
          <input
            type="projector"
            id="projector"
            name="projector"
            class="form-control"
            placeholder="true/false"
            value=""<%= typeof projector != 'undefined' ? projector : '' %>"
          >
        </div>

       
        <div class="form-group">
          <label for="SlotStart">Slot Start</label>
          <input
            type="slotStart"
            id="slotStart"
            name="slotStart"
            class="form-control"
            placeholder="Etc 9:00AM"
            value=""<%= typeof slotStart != 'undefined' ? slotStart : '' %>"
          >
        </div>

        <div class="form-group">
          <label for="SlotEnd">Slot End</label>
          <input
            type="slotEnd"
            id="slotEnd"
            name="slotEnd"
            class="form-control"
            placeholder="Etc 10:20AM"
            value=""<%= typeof slotEnd != 'undefined' ? slotEnd : '' %>"
          >
        </div>
      
       
        

		
       
        
   
        <button type="submit" class="btn btn-info btn-block">
          Add Venue
        </button>
      </form>
	  <p class="lead mt-2">Remove Venue <a href="/settings/rVenue">Here</a></p>
      
    </div>
  </div>
</div>

        

