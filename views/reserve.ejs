<div class ="mt-4 card card-body"  style="border:1px solid black">
    <h1 class="mt-4"  style =" text-align: center"><i class="fas fa-pencil-alt"></i>  Making Reservations</h1>

    <p class="lead mb-1" style =" text-align: center">Available Time Slots :</p>
    <p class="lead mb-1" style =" text-align: center">8:30AM - 9:50AM </p>
    <p class="lead mb-1" style =" text-align: center">10:00AM - 11:20AM </p>
    <p class="lead mb-1" style =" text-align: center">11:30AM - 12:50PM </p>
    <p class="lead mb-1" style =" text-align: center">2:00PM - 3:20PM </p>
    <p class="lead mb-1" style =" text-align: center">3:30PM - 4:50PM </p>
    <p class="lead mb-1" style =" text-align: center">8:00PM - 10:00PM </p>
    <p class="lead mb-1" style =" text-align: center">Note: Starting time and Ending time should correspond to the time slots above </p>
    
</div>
<div class="row mt-3">
    <div class="col-md-6 m-auto">
      <div class="card card-body">
        <% include ./partials/messages %>
        <form id="form" action="/dashboard/reserve" method="POST">
          <a href ="/dashboard/reserve/fVenue" class="btn btn-success btn-block">View Available Venues</a>
        
            <div class="form-group mt-2">
                <label for="venue">Venue</label>
                <select class="custom-select" id="roomNo" name="roomNo">
                <option value=""> Please Select a Venue</option>
                <option value="e1-1-1"<%= typeof roomNo != 'undefined' ? roomNo : '' %>"
                >e1-1-1</option>
                <option value="e1-1-2"<%= typeof roomNo != 'undefined' ? roomNo : '' %>"
                >e1-1-2</option>
                <option value="e1-1-3"<%= typeof roomNo != 'undefined' ? roomNo : '' %>"
                >e1-1-3</option>
                </select>
               
                
            </div>

            
        
        <div class="form-group mt-2">
            
                <label for="reserveStartDate">Start date of reservation</label>
                <input
                    type="date"
                    id="reserveStartDate"
                    name="reserveStartDate"
                    class="form-control"
                    placeholder="dd/mm/yyyy"
                    value="reserveStartDate"<%= typeof reserveStartDate != 'undefined' ? reserveStartDate: '' %>
                />
            
            
        </div>
        
        <div class="form-row">
          <div class="form-group mt-2 col-md-6">
            <label for="reserveStartTime">Start time Of Reservation</label>
            <select class="custom-select" name="reserveStartTime" id="reserveStartTime">
                <option value="">
                    Select Start Time
                  </option>
            <option
                type="reserveStartTime"
                id="reserveStartTime"
                name="reserveStartTime"
                class="form-control"
                
                
                value="8:30AM"<%= typeof reserveStartTime != 'undefined' ? reserveStartTime: '' %>
            >8:30AM</option>
            <option
                type="reserveStartTime"
                id="reserveStartTime"
                name="reserveStartTime"
                class="form-control"
                
                value="reserveStartTime"<%= typeof reserveStartTime != 'undefined' ? reserveStartTime: '' %>
            >10:00AM</option>
            <option
                type="reserveStartTime"
                id="reserveStartTime"
                name="reserveStartTime"
                class="form-control"
                
                value="reserveStartTime"<%= typeof reserveStartTime != 'undefined' ? reserveStartTime: '' %>
            >11:30AM</option>
            <option
                type="reserveStartTime"
                id="reserveStartTime"
                name="reserveStartTime"
                class="form-control"
                
                value="reserveStartTime"<%= typeof reserveStartTime != 'undefined' ? reserveStartTime: '' %>
            >2:00PM</option>
            <option
                type="reserveStartTime"
                id="reserveStartTime"
                name="reserveStartTime"
                class="form-control"
                
                value="reserveStartTime"<%= typeof reserveStartTime != 'undefined' ? reserveStartTime: '' %>
            >3:30PM</option>
            <option
                type="reserveStartTime"
                id="reserveStartTime"
                name="reserveStartTime"
                class="form-control"
                
                value="reserveStartTime"<%= typeof reserveStartTime != 'undefined' ? reserveStartTime: '' %>
            >5:00PM</option>
            <option
                type="reserveStartTime"
                id="reserveStartTime"
                name="reserveStartTime"
                class="form-control"
                
                value="reserveStartTime"<%= typeof reserveStartTime != 'undefined' ? reserveStartTime: '' %>
            >8:00PM</option>
            </select>
        </div>
                <div class="form-group mt-2 col-md-6">
                    <label for="reserveEndTime">End Time Of Reservation </label>
                    <select class="custom-select" name="reserveEndTime" id="reserveEndTime">
                    <option selected="">
                      Select End Time
                    </option>
                    <option
                        type="reserveEndTime"
                        id="reserveEndTime"
                        name="reserveEndTime"
                        class="form-control"
                        
                        value="9:50AM"<%= typeof reserveEndTime != 'undefined' ? reserveEndTime: '' %>
                    >9:50AM</option>
                    <option
                        type="reserveEndTime"
                        id="reserveEndTime"
                        name="reserveEndTime"
                        class="form-control"
                        
                        value="reserveEndTime"<%= typeof reserveEndTime != 'undefined' ? reserveEndTime: '' %>
                    >11:20AM</option>
                    
                    <option
                        type="reserveEndTime"
                        id="reserveEndTime"
                        name="reserveEndTime"
                        class="form-control"
                        
                        value="reserveEndTime"<%= typeof reserveEndTime != 'undefined' ? reserveEndTime: '' %>
                    >12:50PM</option>
                    <option
                        type="reserveEndTime"
                        id="reserveEndTime"
                        name="reserveEndTime"
                        class="form-control"
                        
                        value="reserveEndTime"<%= typeof reserveEndTime != 'undefined' ? reserveEndTime: '' %>
                    >3:20PM</option>
                    <option
                        type="reserveEndTime"
                        id="reserveEndTime"
                        name="reserveEndTime"
                        class="form-control"
                        
                        value="reserveEndTime"<%= typeof reserveEndTime != 'undefined' ? reserveEndTime: '' %>
                    >6:20PM</option>
                    <option
                        type="reserveEndTime"
                        id="reserveEndTime"
                        name="reserveEndTime"
                        class="form-control"
                        
                        value="reserveEndTime"<%= typeof reserveEndTime != 'undefined' ? reserveEndTime: '' %>
                    >10:00PM</option>
                    </select>
                </div>
            </div>

                <div class="form-group mt-2">
                    <label for="purpose">Purpose Of Reservation</label>
                    <input
                        type="purpose"
                        id="purpose"
                        name="purpose"
                        class="form-control"
                        placeholder="Purpose"
                        value=""<%= typeof purpose != 'undefined' ? purpose: '' %>
                    />
                </div>

                <div class="form-row">
                    <div class="form-group mt-2 col-md-6">
                        <a href="/dashboard" class="btn btn-primary btn-block ">Back to Dashboard</a>
                        
                       
                        
                    </div>
        
                    <div class="form-group mt-2 col-md-6">
                        <button type="submit" class="btn btn-info btn-block ">Request venue</button>
                    </div>
                </div>
            

        </form>
    
        
      </div>
    </div>
  </div>
  <script>
      var today = new Date().toISOString().split('T')[0];
  document.getElementsByName("reserveStartDate")[0].setAttribute('min', today);
  </script>

